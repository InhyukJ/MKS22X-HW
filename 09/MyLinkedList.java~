public class MyLinkedList {
    private LNode start, end;
    private int size;

    private class LNode {
	int v;
	LNode next, prev;

	public LNode(int n) {
	    v = n;
	    next = null;
	    prev = null;
	}

	public LNode(int n, LNode node) {
	    v = n;
	    next = node;
	}
    }

    public MyLinkedList() {
	start = null;
	end = null;
	size = 0;
    }

    
